    0% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
    }
    48% {
        -webkit-transform: translateX(0);
        transform: translateX(0);
    }
    to {
        -webkit-transform: translateX(var(--tds-plus--transform-size));
        transform: translateX(var(--tds-plus--transform-size));
    }
}
